<h1>ちゃっと</h1>
<a>man = <%= @man_name %></a>
<a>woman = <%= @woman_name %></a>

<form class="form-horizontal">
	<div class="form-group">
		<label class="col-sm-2 control-label" for="username">なまえ</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" id="username"/>
	    </div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label" for="msgbody">めっせーじ</label>
	    <div class="col-sm-10">
			<input type="text" class="form-control" id="msgbody" />
	    </div>
	</div>
    <div class="form-group">
        <label class="col-sm-2 control-label" for="group_id">ぐるーぷあいでぃー</label>
        <div class="col-sm-10">
<input type="text" class="form-control" id="group_id" value="1" />
        </div>
    </div>
	<div class="form-group">
	    <div class="col-sm-offset-2 col-sm-10">
	    	<input type="button" class="btn btn-primary" id="send" value="送信" />
	    </div>
	</div>
</form>
<div id="chat" class="well" data-uri="<%= request.host %>:<%= request.port %>/websocket">
<% @messages.each do |ms| %>
    <a><%= ms.message %><%= ms.message %></a><br>
<% end %>

</div>

